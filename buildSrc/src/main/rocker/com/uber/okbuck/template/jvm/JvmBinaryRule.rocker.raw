@import java.util.Set
@args (
String mainClassName,
Set excludes
)
@com.uber.okbuck.template.base.BuckRule.template() -> {
@if (mainClassName != null) {
    main_class = '@mainClassName',
}
@if (valid(excludes)) {
    blacklist = [
    @for (exclude : excludes) {
        '@exclude',
    }
    ],
}
@if (mavenCoords != null) {
    maven_coords = '@mavenCoords',
}
    deps = [ ':src_main' ] + lib_deps,
}
